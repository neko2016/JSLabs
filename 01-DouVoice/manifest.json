{
  "manifest_version": 2,
  "name": "Dou Voice",
  "version": "0.1",
  "content_scripts": [
      {
        "matches": [
          "https://www.douban.com/*", "http://www.douban.com/*",
          "https://*.douban.com/*",   "http://*.douban.com/*"
        ],
        "js": ["DouVoice.js"]
      }
  ],
  "browser_action": {
        "default_icon": "douVoice.png"
  }
}